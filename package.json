{
  "name": "newts",
  "version": "1.5.0",
  "description": "",
  "main": "dist/newts.js",
  "bin": {
    "newts": "./dist/newts-cli.js"
  },
  "files": [
    "dist/**/*",
    "_gitignore",
    "jest.config.js",
    "licenses/*"
  ],
  "scripts": {
    "test": "jest",
    "lint": "tslint -p .",
    "autolint": "nodemon -e ts -x \"run-s lint\"",
    "autobuild": "nodemon -w src -w tests -e ts -x \"run-s --silent build\"",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "cp .gitignore _gitignore",
    "prerelease-beta": "run-s build lint test",
    "release-beta": "cross-env BETA=1 VERSION_INCREMENT_STRATEGY=patch run-s \"zarro release-npm\"",
    "prerelease": "run-s build lint test",
    "release": "cross-env VERSION_INCREMENT_STRATEGY=minor run-s \"zarro release-npm\"",
    "zarro": "zarro",
    "start": "ts-node src/newts-cli.ts",
    "download-licenses": "ts-node tools/download-licenses.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fluffynuts/newts"
  },
  "keywords": [
    "typescript",
    "generator",
    "bootstrap",
    "module",
    "library",
    "quick-start"
  ],
  "author": {
    "name": "Davyd McColl",
    "email": "davydm@gmail.com",
    "url": "https://github.com/fluffynuts"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/bent": "^7.0.2",
    "@types/faker": "^4.1.12",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.3",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^14.0.9",
    "@types/rimraf": "^3.0.0",
    "@types/validate-npm-package-name": "^3.0.0",
    "@types/which": "^1.3.2",
    "@types/yargs": "^15.0.5",
    "cpy-cli": "^3.1.1",
    "expect-even-more-jest": "^1.3.0",
    "faker": "^4.1.0",
    "filesystem-sandbox": "^1.12.0",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3",
    "zarro": "^1.55.0"
  },
  "dependencies": {
    "bent": "^7.3.7",
    "chalk": "^4.1.0",
    "inquirer": "^7.2.0",
    "validate-npm-package-name": "^3.0.0",
    "which": "^2.0.2",
    "yargs": "^15.3.1"
  }
}
